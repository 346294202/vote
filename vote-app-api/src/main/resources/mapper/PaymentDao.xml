<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.leoyon.vote.payment.dao.PaymentDao">

	<insert id="add" parameterType="com.leoyon.vote.payment.Payment" >
	INSERT INTO `basic_payment`
(`id`,
`channel`,
`amount`,
`data`,
`status`)
VALUES
(#{id},
#{channel},
#{amount},
#{data},
#{status});
	
	</insert>		
</mapper>
