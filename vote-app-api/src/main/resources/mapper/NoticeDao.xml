<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.leoyon.vote.notice.dao.NoticeDao">

	<select id="list" parameterType="com.leoyon.vote.FindPagedRequest" resultType="com.leoyon.vote.notice.Notice">
	select * from property_notice where `state`=1 and `delete`=1 order by update_time desc
		LIMIT #{page} , #{psize};
	</select>
	
</mapper>
